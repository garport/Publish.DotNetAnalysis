{
  "version": "2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "CodeAnalysisTool",
          "version": "1.0.0",
          "informationUri": "https://example.com",
          "rules": [
            {
              "id": "CA2000",
              "name": "Dispose objects before losing scope",
              "fullDescription": {
                "text": "All IDisposable objects should be disposed of before they go out of scope."
              },
              "defaultConfiguration": {
                "level": "warning"
              }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "CA2000",
          "level": "warning",
          "message": {
            "text": "Dispose objects before losing scope."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "Program.cs",
                  "uriBaseId": "%SRCROOT%"
                },
                "region": {
                  "startLine": 10,
                  "startColumn": 13,
                  "endLine": 10,
                  "endColumn": 28
                }
              }
            }
          ]
        }
      ]
    }
  ]
}
